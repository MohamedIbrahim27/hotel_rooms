{
  "version": 2,
  "builds": [
    {
      "src": "project/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9"
      }
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "command": "./build_files.sh"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ]
}
